{
    "name": "dynamodbinsert",
    "type": "flogo:activity",
    "ref": "github.com/ecarlier-tibco/flogo-components/activity/dynamodbinsert",
    "version": "0.1.0",
    "title": "DynamoDB Insert ERC",
    "description": "Insert an object into Amazon DynamoDB",
    "author": "retgits",
    "homepage": "https://github.com/ecarlier-tibco/flogo-components/tree/master/activity/dynamodbinsert",
    "inputs":[
      {
        "name": "awsAccessKeyID",
        "type": "string",
        "required": false
      },
      {
        "name": "awsSecretAccessKey",
        "type": "string",
        "required": false
      },
      {
        "name": "awsRegion",
        "type": "string",
        "required": true
      },
      {
        "name": "DynamoDBTableName",
        "type": "string",
        "required": true
      },
      {
        "name": "DynamoDBRecord",
        "type": "any",
        "required": true
      }
    ],
    "outputs": [
      {
        "name": "result",
        "type": "any"
      }
    ]
  }
